{
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "jquery": false,
    "es6": true
  },
  "plugins": [
    "babel"
  ],
  "globals": {
    "atom": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "arrowFunctions": true,
      "binaryLiterals": true,
      "blockBindings": true,
      "classes": true,
      "defaultParams": true,
      "destructuring": true,
      "experimentalObjectRestSpread": true,
      "forOf": true,
      "generators": true,
      "modules": true,
      "objectLiteralComputedProperties": true,
      "objectLiteralDuplicatedProperties": true,
      "objectLiteralShorthandMethods": true,
      "objectLiteralShorthandProperties": true,
      "octalLiterals": true,
      "regexUFlag": true,
      "regexYFlag": true,
      "restParams": true,
      "spread": true,
      "superInFunctions": true,
      "templateStrings": true,
      "unicodeCodePointEscapes": true,
      "globalReturn": true,
      "jsx": true
    }
  },
  "extends": "eslint:recommended",
  "rules": {
    "comma-dangle": [
      1,
      "never"
    ],
    "no-cond-assign": 1,
    "no-console": 0,
    "no-constant-condition": 0,
    "no-control-regex": 1,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 1,
    "no-empty": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 1,
    "no-extra-parens": 0,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": [
      2,
      "both"
    ],
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 1,
    "no-obj-calls": 2,
    "no-regex-spaces": 1,
    "no-sparse-arrays": 1,
    "no-unexpected-multiline": 1,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-jsdoc": 2,
    "valid-typeof": 2,
    "accessor-pairs": 2,
    "array-callback-return": 1,
    "block-scoped-var": 2,
    "complexity": [
      1,
      20
    ],
    "curly": [
      2,
      "multi-line"
    ],
    "default-case": 1,
    "dot-location": [
      1,
      "property"
    ],
    "dot-notation": 1,
    "eqeqeq": [
      2,
      "smart"
    ],
    "guard-for-in": 1,
    "no-alert": 2,
    "no-caller": 2,
    "no-case-declarations": 2,
    "no-div-regex": 2,
    "no-else-return": 1,
    "no-empty-pattern": 2,
    "no-eq-null": 0,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 1,
    "no-implicit-coercion": 0,
    "no-implicit-globals": 2,
    "no-implied-eval": 2,
    "no-invalid-this": 0,
    "no-iterator": 2,
    "no-labels": 2,
    "no-lone-blocks": 1,
    "no-loop-func": 2,
    "no-magic-numbers": 0,
    "no-multi-spaces": 2,
    "no-multi-str": 0,
    "no-native-reassign": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-new": 1,
    "no-octal-escape": 2,
    "no-octal": 2,
    "no-param-reassign": 0,
    "no-proto": 2,
    "no-redeclare": [
      2,
      {
        "builtinGlobals": true
      }
    ],
    "no-return-assign": 0,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-throw-literal": 2,
    "no-unmodified-loop-condition": 2,
    "no-unused-expressions": 0,
    "no-useless-call": 1,
    "no-useless-concat": 1,
    "no-void": 2,
    "no-with": 2,
    "radix": 0,
    "vars-on-top": 1,
    "wrap-iife": 2,
    "yoda": 2,
    "init-declarations": 0,
    "no-catch-shadow": 2,
    "no-delete-var": 2,
    "no-label-var": 2,
    "no-shadow-restricted-names": 2,
    "no-shadow": 1,
    "no-undef-init": 1,
    "no-undef": 2,
    "no-undefined": 0,
    "no-unused-vars": 2,
    "no-use-before-define": 0,
    "global-require": 0,
    "handle-callback-err": 1,
    "no-mixed-requires": 0,
    "no-new-require": 1,
    "no-path-concat": 1,
    "no-process-exit": 1,
    "block-spacing": [
      1,
      "never"
    ],
    "brace-style": [
      1,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "camelcase": [
      1,
      {
        "properties": "always"
      }
    ],
    "comma-spacing": 1,
    "comma-style": 1,
    "computed-property-spacing": 1,
    "consistent-this": 0,
    "eol-last": 0,
    "func-names": 0,
    "func-style": 0,
    "id-length": 0,
    "indent": [
      2,
      2,
      {
        "VariableDeclarator": 1,
        "SwitchCase": 0
      }
    ],
    "jsx-quotes": [
      1,
      "prefer-double"
    ],
    "key-spacing": 1,
    "keyword-spacing": 1,
    "linebreak-style": 2,
    "lines-around-comment": 0,
    "max-depth": 1,
    "max-len": 0,
    "max-nested-callbacks": [
      1,
      4
    ],
    "max-params": 0,
    "max-statements": 0,
    "new-parens": 0,
    "newline-after-var": 0,
    "no-array-constructor": 1,
    "no-bitwise": 1,
    "no-continue": 1,
    "no-inline-comments": 0,
    "no-lonely-if": 1,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": 0,
    "no-negated-condition": 1,
    "no-nested-ternary": 0,
    "no-new-object": 1,
    "no-plusplus": 0,
    "no-whitespace-before-property": 2,
    "no-spaced-func": 2,
    "no-ternary": 0,
    "no-trailing-spaces": 0,
    "no-underscore-dangle": 0,
    "no-unneeded-ternary": 2,
    "one-var": 0,
    "operator-assignment": 1,
    "operator-linebreak": 1,
    "padded-blocks": [
      1,
      "never"
    ],
    "quote-props": [
      1,
      "as-needed"
    ],
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ],
    "require-jsdoc": [
      0,
      {
        "require": {
          "FunctionDeclaration": true,
          "MethodDefinition": true,
          "ClassDeclaration": true
        }
      }
    ],
    "semi-spacing": 1,
    "semi": 2,
    "sort-vars": [
      1,
      {
        "ignoreCase": true
      }
    ],
    "sort-imports": [
      1,
      {
        "ignoreCase": true,
        "ignoreMemberSort": true
      }
    ],
    "template-curly-spacing": [
      1,
      "never"
    ],
    "space-before-blocks": [
      1,
      "always"
    ],
    "space-before-function-paren": [
      1,
      "never"
    ],
    "space-in-parens": [
      1,
      "never"
    ],
    "space-infix-ops": 1,
    "space-unary-ops": 1,
    "spaced-comment": [
      1,
      "always"
    ],
    "wrap-regex": 1,
    "arrow-body-style": 0,
    "arrow-spacing": [
      1,
      {
        "before": true,
        "after": true
      }
    ],
    "constructor-super": 2,
    "no-class-assign": 2,
    "no-confusing-arrow": 1,
    "no-const-assign": 2,
    "no-dupe-class-members": 2,
    "no-new-symbol": 2,
    "no-this-before-super": 2,
    "no-var": 1,
    "no-useless-constructor": 2,
    "prefer-arrow-callback": 1,
    "prefer-const": 1,
    "prefer-reflect": 1,
    "prefer-rest-params": 1,
    "prefer-spread": 1,
    "prefer-template": 1,
    "require-yield": 2,
    "yield-star-spacing": 0,
    "babel/generator-star-spacing": 0,
    "babel/new-cap": 0,
    "babel/array-bracket-spacing": [
      1,
      "never"
    ],
    "babel/object-curly-spacing": [
      1,
      "never"
    ],
    "babel/object-shorthand": 1,
    "babel/arrow-parens": [
      1,
      "always"
    ],
    "babel/no-await-in-loop": 1
  }
}
